programa {
  funcao inicio() {
    inteiro a , b , soma , sub , div , mult

    escreva ("digite o valor de a:")
    leia (a)
    escreva("digite o valor de b:")
    leia(b)
    soma = a+b 
    sub = a-b 
    div = a/b 
    mult = a*b 
    escreva ("\n Soma =" , soma )
    escreva ("\n subtração =" , sub)
    escreva ("\n divisão =", div)
    escreva ("\n multiplicação =" , mult )

      }
}
